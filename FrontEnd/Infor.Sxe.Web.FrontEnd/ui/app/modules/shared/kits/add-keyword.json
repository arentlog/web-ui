        {
          "id": 4,
          "type": "BTN",
          "subType": "MODAL_SEC",
          "label": "global.cancel",
          "eventClick": "kitKa.cancel()"
        },
        {
          "id": 5,
          "type": "BTN",
          "subType": "MODAL_PRI",
          "label": "global.ok",
          "submitForm": true,
          "eventValidated": "kitKa.submit()"
        }
      ]
