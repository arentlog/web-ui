{
  "viewId": "twl-twlat-master",
  "id": 1,
  "type": "VIEW",
  "subType": "MASTER_LIST",
  "children": [
    {
      "id": 2,
      "type": "MOD_CNTR",
      "controls": {
        "toolbar": {
          "id": 3,
          "type": "TOOLBAR",
          "label": "title.twlat",
          "subLabel": "{{base.getSubTitle()}}",
          "children": []
        },
        "content": {
          "id": 4,
          "type": "MOD_CONTENT",
          "children": [
            {
              "type": "GRID",
              "id": 5,
              "controlRef": "base.grid",
              "model": "base.dataset",
              "options": {
                "showCheckboxes": true,
                "showDrilldown": true,
                "selectable": "multiple",
                "events": {
                  "drilldown": "mst.drilldown()"
                }
              },
              "controls": {
                "toolbar": {
                  "id": 6,
                  "type": "TOOLBAR",
                  "label": "global.stations",
                  "children": [
                    {
                      "id": 7,
                      "type": "BTN",
                      "subType": "",
                      "icon": "icon-new-document",
                      "label": "global.new",
                      "eventClick": "mst.create()",
                      "conditionDisabled": "base.grid.isAnyRowSelected() || ((!base.criteria.whNum) && (!base.criteriaUsed.whNum))",
                      "securityLevel": 4
                    },
                    {
                      "id": 8,
                      "type": "BTN",
                      "subType": "",
                      "icon": "icon-edit",
                      "label": "global.edit",
                      "eventClick": "mst.edit()",
                      "conditionDisabled": "!base.grid.isOneRowSelected()",
                      "securityLevel": 3
                    },
                    {
                      "id": 9,
                      "type": "BTN",
                      "subType": "",
                      "icon": "icon-delete",
                      "label": "global.delete",
                      "eventClick": "mst.delete()",
                      "conditionDisabled": "base.grid.isNoRowSelected()",
                      "securityLevel": 5
                    }
                  ]
                }
              },
              "children": [
                {
                  "id": 12,
                  "type": "GRID_COL",
                  "subType": "TEXT",
                  "model": "coNum",
                  "label": "global.company"
                },
                {
                  "id": 13,
                  "type": "GRID_COL",
                  "subType": "TEXT",
                  "model": "whNum",
                  "label": "global.warehouse"
                },
                {
                  "id": 14,
                  "type": "GRID_COL",
                  "subType": "TEXT",
                  "model": "stnNum",
                  "label": "global.station"
                },
                {
                  "id": 18,
                  "type": "GRID_COL",
                  "subType": "DROP_DOWN",
                  "model": "stnType",
                  "label": "global.type",
                  "type3": "SET",
                  "metaGroup": "TWL",
                  "meta": "StationType"
                }
              ]
            }
          ]
        }
      }
    }
