{
  "viewId": "twl-twlcrc-detail",
  "id": 1,
  "type": "VIEW",
  "subType": "DETAIL",
  "children": [
    {
      "id": 2,
      "type": "FORM",
      "conditionReadonly": "base.isDetail()",
      "children": [
        {
          "id": 3,
          "type": "MOD_CNTR",
          "controls": {
            "toolbar": {
              "id": 4,
              "type": "TOOLBAR",
              "label": "menu.twlcrc",
              "subLabel": "{{dtl.getSubTitle()}}",
              "controls": {
                "navBtns": {
                  "id": 5,
                  "type": "BTN_SET",
                  "subType": "TBAR_NAV",
                  "children": [
                    {
                      "id": 6,
                      "type": "BTN",
                      "subType": "ICON",
                      "label": "special.button.back",
                      "icon": "icon-left-arrow",
                      "stateRef": "^.master"
                    }
                  ]
                }
              },
              "children": [
                {
                  "id": 7,
                  "type": "BTN",
                  "subType": "",
                  "icon": "icon-new-document",
                  "label": "global.new",
                  "eventClick": "dtl.create()",
                  "conditionDisabled": "!base.isDetail() || ((!base.criteria.whNum) && (!base.criteriaUsed.whNum))",
                  "securityLevel": 4
                },
                {
                  "id": 8,
                  "type": "BTN",
                  "subType": "",
                  "icon": "icon-edit",
                  "label": "global.edit",
                  "stateRef": ".edit",
                  "conditionDisabled": "!base.isDetail()",
                  "securityLevel": 3
                },
                {
                  "id": 9,
                  "type": "BTN",
                  "subType": "",
                  "icon": "icon-delete",
                  "label": "global.delete",
                  "eventClick": "dtl.delete()",
                  "conditionDisabled": "!base.isDetail()",
                  "securityLevel": 5
                },
                {
                  "id": 10,
                  "type": "BTN",
                  "subType": "",
                  "icon": "icon-save",
                  "label": "global.save",
                  "submitForm": true,
                  "eventValidated": "dtl.submit()",
                  "conditionDisabled": "base.isDetail()",
                  "securityLevel": 3
                },
                {
                  "id": 11,
                  "type": "BTN",
                  "subType": "",
                  "icon": "icon-reset",
                  "label": "global.reset",
                  "eventClick": "dtl.reset()",
                  "conditionDisabled": "base.isDetail()"
                },
                {
                  "id": 12,
                  "type": "BTN",
                  "subType": "",
                  "icon": "icon-cancel",
                  "label": "global.cancel",
                  "eventClick": "dtl.cancel()",
                  "conditionDisabled": "base.isDetail()"
                }
              ]
            },
            "content": {
              "id": 13,
              "type": "MOD_CONTENT",
              "children": [
                {
                  "id": 14,
                  "type": "EXP_AREA",
                  "label": "global.general",
                  "children": [
                    {
                      "id": 15,
                      "type": "ROW",
                      "children": [
                        {
                          "id": 16,
                          "type": "COL",
                          "subType": "1",
                          "children": [
                            {
                              "id": 17,
                              "type": "FIELD",
                              "subType": "TEXT_BOX",
                              "model": "dtl.twlcrc.coNum",
                              "label": "global.company",
                              "required": true,
                              "readonly": true
                            },
                            {
                              "id": 18,
                              "type": "FIELD",
                              "subType": "TEXT_BOX",
                              "model": "dtl.twlcrc.whNum",
                              "label": "global.warehouse",
                              "required": true,
                              "readonly": true
                            },
                            {
                              "id": 19,
                              "type": "FIELD",
                              "subType": "CHECKBOX",
                              "model": "dtl.twlcrc.rowStatus",
                              "label": "global.active"
                            },
                            {
                              "id": 20,
                              "type": "FIELD",
                              "subType": "TEXT_BOX",
                              "model": "dtl.twlcrc.rtnCategory",
                              "label": "global.return.category",
                              "required": true,
                              "maxLength": 4,
                              "size": "XS"
                            },
                            {
                              "id": 21,
                              "type": "FIELD",
                              "subType": "TEXT_BOX",
                              "model": "dtl.twlcrc.description",
                              "label": "global.description",
                              "maxLength": 30
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          }
        }
      ]
    }
  ]
}