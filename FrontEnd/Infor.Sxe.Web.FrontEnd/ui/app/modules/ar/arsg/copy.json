{
  "viewId": "ar-arsg-copy",
  "id": 1,
  "type": "VIEW",
  "subType": "DETAIL",
  "children": [
    {
      "id": 2,
      "type": "FORM",
      "children": [
        {
          "id": 3,
          "type": "MOD_CNTR",
          "controls": {
            "toolbar": {
              "id": 4,
              "type": "TOOLBAR",
              "label": "global.master.group",
              "subLabel": "{{cpy.groupCopy.fromgroupid + ' | ' + cpy.fromname}}",
              "children": [
                {
                  "id": 11,
                  "type": "BTN",
                  "subType": "TER",
                  "label": "global.ok",
                  "icon": "icon-confirm",
                  "submitForm": true,
                  "eventValidated": "cpy.submit()",
                  "securityLevel": 4
                },
                {
                  "id": 12,
                  "type": "BTN",
                  "subType": "TER",
                  "label": "global.cancel",
                  "icon": "icon-cancel",
                  "eventClick": "cpy.cancel()"
                }
              ]
            },
            "content": {
              "id": 5,
              "type": "MOD_CONTENT",
              "children": [
                {
                  "id": 6,
                  "type": "ROW",
                  "children": [
                    {
                      "id": 8,
                      "type": "COL",
                      "subType": "1/2",
                      "children": [
                        {
                          "id": 9,
                          "type": "FIELD",
                          "subType": "TEXT_BOX",
                          "model": "cpy.groupCopy.togroupid",
                          "label": "global.group.number",
                          "maxLength": 12,
                          "required": true
                        }
                      ]
                    },
                    {
                      "id": 7,
                      "type": "COL",
                      "subType": "1/2",
                      "children": [
                        {
                          "id": 10,
                          "type": "FIELD",
                          "subType": "TEXT_BOX",
                          "model": "cpy.groupCopy.name",
                          "label": "global.name",
                          "maxLength": 30,
                          "required": true
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          }
        }
      ]
    }
  ]
}