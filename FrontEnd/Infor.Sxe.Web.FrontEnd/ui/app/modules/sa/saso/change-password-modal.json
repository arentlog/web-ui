{
  "viewId": "sa-saso-changepassword",
  "id": 1,
  "type": "MODAL",
  "subType": "MODAL",
  "label": "global.change.password",
  "controls": {
    "body": {
      "id": 2,
      "type": "MODAL_BODY",
      "children": [
        {
          "id": 6,
          "type": "FIELD",
          "subType": "PASSWORD",
          "model": "drldwnCpm.oldPassword",
          "label": "global.old.password",
          "maxLength": 16,
          "autoFocus": true
        },
        {
          "id": 9,
          "type": "FIELD",
          "subType": "PASSWORD",
          "model": "drldwnCpm.newPassword",
          "label": "global.new.password",
          "maxLength": 16
        },
        {
          "id": 10,
          "type": "FIELD",
          "subType": "PASSWORD",
          "model": "drldwnCpm.confirmNewPassword",
          "label": "global.confirm.password",
          "maxLength": 16
        }
      ]
    },
    "btnSet": {
      "id": 3,
      "type": "BTN_SET",
      "subType": "MODAL",
      "children": [
        {
          "id": 4,
          "type": "BTN",
          "subType": "MODAL_SEC",
          "label": "global.cancel",
          "eventClick": "drldwnCpm.cancel()"
        },
        {
          "id": 5,
          "type": "BTN",
          "subType": "MODAL_PRI",
          "label": "global.ok",
          "submitForm": true,
          "eventValidated": "drldwnCpm.ok()"
        }
      ]
    }
  }
}